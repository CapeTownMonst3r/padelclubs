{
  "version": 2,
  "builds": [
    {
      "src": "build/server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/padel-logo.svg",
      "dest": "/build/client/padel-logo.svg"
    },
    {
      "src": "/favicon.ico",
      "dest": "/build/client/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/build/server/index.js"
    }
  ]
}
